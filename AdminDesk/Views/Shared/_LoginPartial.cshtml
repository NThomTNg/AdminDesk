@using Microsoft.AspNetCore.Identity
@inject SignInManager<IdentityUser> SignInManager
@inject UserManager<IdentityUser> UserManager
<style>

</style>

<ul class="navbar-nav">
    @if (SignInManager.IsSignedIn(User))
    {
        <li class="nav-item right">
            <a class="nav-link" asp-area="Identity" asp-page="/Account/Manage/Index" title="Manage">Min side</a>
        </li>
        <li class="nav-item right">
            <form class="form-inline" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "Home", new { area = "" })">
                <button type="submit" class="nav-link btn btn-link text-dark">Logg ut</button>
            </form>
        </li>
    }
    else
    {
        <li class="nav-item right">
            <a class="nav-link" asp-area="Identity" asp-page="/Account/Login">Logg inn</a>
        </li>
    }
</ul>

